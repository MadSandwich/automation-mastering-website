{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["src/assets/*"],
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@utils/*": ["src/utils/*"],
			"@src/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	},
	"extends": "astro/tsconfigs/strict"
}
